            <param name="level" value="$parameters?pstyle(.)//w:outlineLvl/@w:val/number() + 1"/>
        </model>
                    <model behaviour="paragraph"/>
                </elementSpec>
                <elementSpec ident="r" mode="add">
                    <model predicate="$parameters?cstyle(.)/w:rPr/w:i" behaviour="inline">
            <outputRendition xml:space="preserve">
            font-style: italic;
            </outputRendition>
        </model>
                    <model predicate="$parameters?cstyle(.)/w:rPr/w:b" behaviour="inline">
            <outputRendition xml:space="preserve">
            font-weight: bold;
            </outputRendition>
        </model>
                    <model behaviour="inline"/>
